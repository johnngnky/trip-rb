{
  "name": "tripetto-example-react",
  "title": "Tripetto collector example (React)",
  "version": "1.0.0",
  "description": "Demo for implementing the Tripetto collector using React.",
  "license": "MIT",
  "private": true,
  "homepage": "https://gitlab.com/tripetto/examples/react",
  "author": {
    "name": "Mark van den Brink",
    "email": "mark@vandenbr.ink",
    "url": "http://mark.vandenbr.ink"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/tripetto/examples/react.git"
  },
  "bugs": {
    "url": "https://gitlab.com/tripetto/examples/react/issues"
  },
  "scripts": {
    "start": "tripetto ./static/demo.json",
    "format": "prettier --tab-width 4 --print-width 140 --write \"src/**/*.ts\" \"src/**/*.tsx\" \"webpack.config.js\"",
    "lint": "tslint -c ./tslint.json ./src/**/*.ts",
    "test": "webpack-dev-server --mode development",
    "make": "npm run format && npm run lint && webpack --mode production"
  },
  "dependencies": {},
  "devDependencies": {
    "@marblecore/tslint-configuration": "^5.9.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/superagent": "^3.8.0",
    "prettier": "^1.12.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "superagent": "^3.8.3",
    "tripetto": "^0.4.6",
    "tripetto-block-checkbox": "^1.0.21",
    "tripetto-block-checkboxes": "^1.0.14",
    "tripetto-block-dropdown": "^1.0.13",
    "tripetto-block-email": "^1.0.14",
    "tripetto-block-number": "^1.0.12",
    "tripetto-block-password": "^1.0.12",
    "tripetto-block-radiobuttons": "^1.0.12",
    "tripetto-block-text": "^1.0.12",
    "tripetto-block-textarea": "^1.0.11",
    "tripetto-block-url": "^1.0.11",
    "tripetto-collector": "^0.4.25",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.4"
  },
  "tripetto": {
    "blocks": [
      "tripetto-block-checkbox",
      "tripetto-block-checkboxes",
      "tripetto-block-dropdown",
      "tripetto-block-email",
      "tripetto-block-number",
      "tripetto-block-password",
      "tripetto-block-radiobuttons",
      "tripetto-block-text",
      "tripetto-block-textarea",
      "tripetto-block-url"
    ],
    "noFurtherLoading": true
  }
}
